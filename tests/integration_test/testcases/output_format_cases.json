{
  "testSuite": {
    "name": "Output Format Integration Tests",
    "description": "Integration tests for output format functionality (JSON, JSONL, ObjectType)",
    "version": "1.0.0"
  },
  "testCases": [
    {
      "name": "test_json_output_format",
      "description": "Test JSON output format parsing and validation",
      "enabled": true,
      "timeout": 30,
      "parameters": {
        "query": "Generate a simple user profile",
        "history": [],
        "variables": {}
      },
      "dolphinLangPath": "dolphins/output_format_json.dph",
      "expectedResult": {
        "contentKeywords": ["John Doe", "johndoe@example.com"],
        "excludeKeywords": ["error", "failed"],
        "tools": [],
        "outputFormat": null,
        "errorExpected": false,
        "variableChecks": [
          {
            "variableName": "result",
            "expectedContent": "John Doe"
          },
          {
            "variableName": "result",
            "expectedContent": "answer"
          }
        ]
      }
    },
    {
